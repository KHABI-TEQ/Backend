{
	"name": "khab-tech-backend",
	"version": "0.0.1",
	"scripts": {
		"dev": "nodemon --watch src --exec ts-node src/index.ts",
		"start": "node -r ./dist ",
		"build": "tsc -p .",
		"lint": "tslint --project \"tsconfig.json\"",
		"test": "jest --testTimeout=10000"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts, html",
		"ignore": [
			"src/public"
		],
		"exec": "ts-node -r tsconfig-paths/register ./src"
	},
	"_moduleAliases": {
		"@controllers": "dist/controllers",
		"@entities": "dist/entities",
		"@shared": "dist/shared",
		"@server": "dist/Server"
	},
	"dependencies": {
		"@types/multer": "^1.4.12",
		"axios": "^1.7.9",
		"bcryptjs": "^2.4.3",
		"cloudinary": "^2.5.1",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^8.6.0",
		"express": "^4.21.2",
		"express-async-errors": "^3.1.1",
		"fuse.js": "^7.1.0",
		"google-auth-library": "^9.15.1",
		"helmet": "^4.6.0",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"moment": "^2.30.1",
		"mongoose": "^5.13.23",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"nodemailer": "^6.10.0"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/find": "^0.2.4",
		"@types/helmet": "^0.0.48",
		"@types/joi": "^17.2.2",
		"@types/jsonfile": "^6.1.4",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/mongoose": "^5.11.96",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.14.0",
		"@types/node-cron": "^3.0.11",
		"@types/nodemailer": "^6.4.17",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"eslint": "^7.32.0",
		"find": "^0.3.0",
		"fs-extra": "^9.1.0",
		"git-cz": "^4.9.0",
		"jsonfile": "^6.1.0",
		"node-cron": "^3.0.3",
		"nodemon": "^2.0.22",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^3.15.0",
		"typescript": "^4.9.5"
	}
}
